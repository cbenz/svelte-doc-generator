    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/commander": "^2.12.2",
    "@types/dom-parser": "^0.1.0",
    "@types/estree": "0.0.42",
    "@types/fs-extra": "^8.0.1",
    "@types/md5": "^2.1.33",
    "@types/node": "^13.5.0",

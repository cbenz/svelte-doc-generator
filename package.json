    "@types/md5": "^2.1.33",
    "@types/node": "^13.5.0",
    "@types/yargs": "^15.0.1",
    "autoprefixer": "^9.7.4",
    "babel-runtime": "^6.26.0",
    "cssnano": "^4.1.10",
    "estree-walker": "^1.0.1",
    "file-loader": "^5.0.2",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-postcss": "^8.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",

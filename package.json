{
  "type": "module",
  "name": "svelte-doc-generator",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "check": "./node_modules/.bin/tsc --noEmit",
    "check:watch": "npm run check -- --watch",
    "system:watch": "./node_modules/.bin/tsc -w",
    "system:build": "npm run build:types && npm run build:js",
    "system:build:types": "./node_modules/.bin/tsc --emitDeclarationOnly",
    "system:build:js": "babel system --out-dir dist/system --extensions ',.ts,.tsx' --source-maps inline",
    "source:watch": "./node_modules/.bin/webpack --watch --mode development",
    "source:build": "./node_modules/.bin/webpack --mode production"
  },
  "bin": {
    "svelte-doc-generator": "./dist/system/cli.js"
  },
  "author": "Pavel Kondratenko <iam@kondratenko.me>",
  "license": "ISC",

{
  "name": "your-svelte-library",
  "version": "0.1.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "npm run docs:generate && npm run webpack:start",
    "build": "npm run docs:generate && npm run webpack:build",
    "webpack:start": "node ./node_modules/.bin/webpack-dev-server --mode development",
    "webpack:build": "node ./node_modules/.bin/webpack --mode production",
    "docs:watch": "node ./node_modules/.bin/svelte-doc-generator watch --library ./src/components --target ./site/library",
    "docs:generate": "node ./node_modules/.bin/svelte-doc-generator generate --library ./src/components --target ./site/library"
  },
  "author": "Ivan",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "postcss": "7.0.18",
    "postcss-automath": "1.0.1",
    "postcss-each": "0.10.0",
    "postcss-for": "2.1.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.2",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.6.0",
    "postcss-simple-vars": "5.0.2",
    "raw-loader": "3.1.0",
    "style-loader": "0.23.1",
    "svelte": "3.12.1",
    "svelte-loader": "2.13.4",
    "svelte-preprocess": "2.15.0",
    "svelte-routing": "1.2.0",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.3.3",
    "webpack-dev-server": "3.7.1"
  }
}
